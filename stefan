#
w1 = float(input ("Введите w1 = "))
w2 = float(input ("Введите w2 = "))
w3 = float(input ("Введите w3 = "))
w4 = float(input ("Введите w4 = "))
tran1=(w1*x1)+(w2*xx1)+(w3*xxx1)+(w4*xxxx1)
tran2=(w1*x2)+(w2*xx2)+(w3*xxx2)+(w4*xxxx2)
tran3=(w1*x3)+(w2*xx3)+(w3*xxx3)+(w4*xxxx3)
print("\n"+"A="+str(tran1)+"\n"+"B="+str(tran2)+"\n"+"C="+str(tran3)+"\n")
