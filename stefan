a1 = float(input ("Введите первое число транспорта А: "))
b1 = float(input ("Введите первое число транспорта B: "))
c1 = float(input ("Введите первое число транспорта C: "))
proverka = float(input ("\n"+"Первая характеристика - stymulanta - введите 1, если destymulanta - введите 2, nominanta - 3 "+"\n"))
if proverka==1:
	maxx=max(a1,b1,c1)
	minn=min(a1,b1,c1)
	x1=(a1-minn)/(maxx-minn)
	x2=(b1-minn)/(maxx-minn)
	x3=(c1-minn)/(maxx-minn)
	print("\n"+"x1="+str(x1)+"\n"+"x2="+str(x2)+"\n"+"x3="+str(x3)+"\n")
elif proverka==2:
	maxx=max(a1,b1,c1)
	minn=min(a1,b1,c1)
	x1=(maxx-a1)/(maxx-minn)
	x2=(maxx-b1)/(maxx-minn)
	x3=(maxx-c1)/(maxx-minn)
	print("\n"+"x1="+str(x1)+"\n"+"x2="+str(x2)+"\n"+"x3="+str(x3)+"\n")
else:
	nominan=float(input ("Введите идеальное значение для номинанты: "))
	maxx=max(a1,b1,c1,nominan)
	minn=min(a1,b1,c1,nominan)
	if nominan>a1:
		x1=(a1-minn)/(nominan-minn)
	else:
		x1=(maxx-a1)/(maxx-nominan)
	if nominan>b1:
		x2=(b1-minn)/(nominan-minn)
	else:
		x2=(maxx-b1)/(maxx-nominan)
	if nominan>c1:
		x3=(c1-minn)/(nominan-minn)
	else:
		x3=(maxx-c1)/(maxx-nominan)
	print("\n"+"x1="+str(x1)+"\n"+"x2="+str(x2)+"\n"+"x3="+str(x3)+"\n")
