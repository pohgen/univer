a1 = float(input ("Введите второе число транспорта А: "))
b1 = float(input ("Введите второе число транспорта B: "))
c1 = float(input ("Введите второе число транспорта C: "))
proverka = float(input ("\n"+"Вторая характеристика - stymulanta - введите 1, если destymulanta - введите 2, nominanta - 3 "+"\n"))
if proverka==1:
	maxx=max(a1,b1,c1)
	minn=min(a1,b1,c1)
	xx1=(a1-minn)/(maxx-minn)
	xx2=(b1-minn)/(maxx-minn)
	xx3=(c1-minn)/(maxx-minn)
	print("\n"+"x1="+str(xx1)+"\n"+"x2="+str(xx2)+"\n"+"x3="+str(xx3)+"\n")
elif proverka==2:
	maxx=max(a1,b1,c1)
	minn=min(a1,b1,c1)
	xx1=(maxx-a1)/(maxx-minn)
	xx2=(maxx-b1)/(maxx-minn)
	xx3=(maxx-c1)/(maxx-minn)
	print("\n"+"x1="+str(xx1)+"\n"+"x2="+str(xx2)+"\n"+"x3="+str(xx3)+"\n")
else:
	nominan=float(input ("Введите идеальное значение для номинанты: "))
	maxx=max(a1,b1,c1,nominan)
	minn=min(a1,b1,c1,nominan)
	if nominan>a1:
		xx1=(a1-minn)/(nominan-minn)
	else:
		xx1=(maxx-a1)/(maxx-nominan)
	if nominan>b1:
		xx2=(b1-minn)/(nominan-minn)
	else:
		xx2=(maxx-b1)/(maxx-nominan)
	if nominan>c1:
		xx3=(c1-minn)/(nominan-minn)
	else:
		xx3=(maxx-c1)/(maxx-nominan)
	print("\n"+"x1="+str(xx1)+"\n"+"x2="+str(xx2)+"\n"+"x3="+str(xx3)+"\n")
