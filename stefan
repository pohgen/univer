a1 = float(input ("Введите четвертое число транспорта А: "))
b1 = float(input ("Введите четвертое число транспорта B: "))
c1 = float(input ("Введите четвертое число транспорта C: "))
proverka = float(input ("\n"+"Четвертая характеристика - stymulanta - введите 1, если destymulanta - введите 2, nominanta - 3 "+"\n"))
if proverka==1:
	maxx=max(a1,b1,c1)
	minn=min(a1,b1,c1)
	xxxx1=(a1-minn)/(maxx-minn)
	xxxx2=(b1-minn)/(maxx-minn)
	xxxx3=(c1-minn)/(maxx-minn)
	print("\n"+"x1="+str(xxxx1)+"\n"+"x2="+str(xxxx2)+"\n"+"x3="+str(xxxx3)+"\n")
elif proverka==2:
	maxx=max(a1,b1,c1)
	minn=min(a1,b1,c1)
	xxxx1=(maxx-a1)/(maxx-minn)
	xxxx2=(maxx-b1)/(maxx-minn)
	xxxx3=(maxx-c1)/(maxx-minn)
	print("\n"+"x1="+str(xxxx1)+"\n"+"x2="+str(xxxx2)+"\n"+"x3="+str(xxxx3)+"\n")
else:
	nominan=float(input ("Введите идеальное значение для номинанты: "))
	maxx=max(a1,b1,c1,nominan)
	minn=min(a1,b1,c1,nominan)
	if nominan>a1:
		xxxx1=(a1-minn)/(nominan-minn)
	else:
		xxxx1=(maxx-a1)/(maxx-nominan)
	if nominan>b1:
		xxxx2=(b1-minn)/(nominan-minn)
	else:
		xxxx2=(maxx-b1)/(maxx-nominan)
	if nominan>c1:
		xxxx3=(c1-minn)/(nominan-minn)
	else:
		xxxx3=(maxx-c1)/(maxx-nominan)
	print("\n"+"x1="+str(xxxx1)+"\n"+"x2="+str(xxxx2)+"\n"+"x3="+str(xxxx3)+"\n")
