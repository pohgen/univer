a1 = float(input ("Введите третье число транспорта А: "))
b1 = float(input ("Введите третье число транспорта B: "))
c1 = float(input ("Введите третье число транспорта C: "))
proverka = float(input ("\n"+"Третья характеристика - stymulanta - введите 1, если destymulanta - введите 2, nominanta - 3"+"\n"))
if proverka==1:
	maxx=max(a1,b1,c1)
	minn=min(a1,b1,c1)
	xxx1=(a1-minn)/(maxx-minn)
	xxx2=(b1-minn)/(maxx-minn)
	xxx3=(c1-minn)/(maxx-minn)
	print("\n"+"x1="+str(xxx1)+"\n"+"x2="+str(xxx2)+"\n"+"x3="+str(xxx3)+"\n")
elif proverka==2:
	maxx=max(a1,b1,c1)
	minn=min(a1,b1,c1)
	xxx1=(maxx-a1)/(maxx-minn)
	xxx2=(maxx-b1)/(maxx-minn)
	xxx3=(maxx-c1)/(maxx-minn)
	print("\n"+"x1="+str(xxx1)+"\n"+"x2="+str(xxx2)+"\n"+"x3="+str(xxx3)+"\n")
else:
	nominan=float(input ("Введите идеальное значение для номинанты: "))
	maxx=max(a1,b1,c1,nominan)
	minn=min(a1,b1,c1,nominan)
	if nominan>a1:
		xxx1=(a1-minn)/(nominan-minn)
	else:
		xxx1=(maxx-a1)/(maxx-nominan)
	if nominan>b1:
		xxx2=(b1-minn)/(nominan-minn)
	else:
		xxx2=(maxx-b1)/(maxx-nominan)
	if nominan>c1:
		xxx3=(c1-minn)/(nominan-minn)
	else:
		xxx3=(maxx-c1)/(maxx-nominan)
	print("\n"+"x1="+str(xxx1)+"\n"+"x2="+str(xxx2)+"\n"+"x3="+str(xxx3)+"\n")
